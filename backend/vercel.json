{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/server.ts"
        }
    ]
}